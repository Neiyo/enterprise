    <select id="ucoinStatistical" resultType="java.util.Map">
        SELECT
        ysh_enterprise_info.enterprise_name AS enterpriseName,
        (
        SELECT
        count(*)
        FROM
        chinapost_customer
        WHERE
        enterprise_id = ysh_enterprise_info.enterprise_id
        AND DATE_FORMAT(create_time, '%Y-%m-%d') = date_sub(curdate(), INTERVAL 1 DAY)
        ) addMemberCount,
        (
        SELECT
        count(*)
        FROM
        ysh_custom_ucion_history
        WHERE
        enterprise_id = ysh_enterprise_info.enterprise_id
        AND type = 'ENTERPRISE_GRAND'
        AND DATE_FORMAT(create_time, '%Y-%m-%d') = date_sub(curdate(), INTERVAL 1 DAY)
        ) sendUcoinCount,
        (
        SELECT
        sum(market_price)
        FROM
        ysh_custom_ucion_history
        WHERE
        enterprise_id = ysh_enterprise_info.enterprise_id
        AND type = 'ENTERPRISE_GRAND'
        AND DATE_FORMAT(create_time, '%Y-%m-%d') = date_sub(curdate(), INTERVAL 1 DAY)
        ) marketUcoinSum,
        (
        SELECT
        sum(sales_price)
        FROM
        ysh_custom_ucion_history
        WHERE
        enterprise_id = ysh_enterprise_info.enterprise_id
        AND type = 'ENTERPRISE_GRAND'
        AND DATE_FORMAT(create_time, '%Y-%m-%d') = date_sub(curdate(), INTERVAL 1 DAY)
        ) salesUcoinSum,
        (
        SELECT
        sum(price)
        FROM
        ysh_custom_ucion_history
        WHERE
        enterprise_id = ysh_enterprise_info.enterprise_id
        AND type = 'ENTERPRISE_GRAND'
        AND DATE_FORMAT(create_time, '%Y-%m-%d') = date_sub(curdate(), INTERVAL 1 DAY)
        ) ucoinSum,
        (
        SELECT
        count(*)
        FROM
        np_order
        WHERE
        enterprise_id = ysh_enterprise_info.enterprise_id
        AND DATE_FORMAT(create_time, '%Y-%m-%d') = date_sub(curdate(), INTERVAL 1 DAY)
        ) orderCount
        FROM
        ysh_enterprise_info
        WHERE
        (
        SELECT

        END
        FROM
        ysh_enterprise_func
        WHERE
        id = ysh_enterprise_info.enterprise_id
        ) = 1
        GROUP BY
        enterprise_id;
    </select>
