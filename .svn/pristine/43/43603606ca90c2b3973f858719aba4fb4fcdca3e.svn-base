package com.ylife.exception;

/**
 * Created by Administrator on 2016/4/11.
 */
public class AuthoritionException extends RuntimeException {

    public static final String message = "权限验证错误。";

    public AuthoritionException() {
        super(message);
    }

    public AuthoritionException(String message) {
        super(message);
    }

    public AuthoritionException(String message, Throwable cause) {
        super(message, cause);
    }

    public AuthoritionException(Throwable cause) {
        super(cause);
    }

}
