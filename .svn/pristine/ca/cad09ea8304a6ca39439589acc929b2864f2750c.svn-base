package com.ylife.order.model;

/**
 * Created by InThEnd on 2016/5/12.
 * 退单原因
 */
public enum CreditOrderReason {

    /**
     * *****退货原因*****
     */

    DONT_WANT_BUY ("1","不想买了"),
    WRONG_INFORMATION ("2","收货人信息有误"),
    GOODS_DAMAGE ("3","商品损坏"),
    GOODS_DESCERROR ("5","收到商品与描述不符"),
    NOT_Delivery_Timed  ("6","未按指定时间发货"),
    OTHERS ("4","其他");

    private String code;

    private String tag;

    CreditOrderReason(String code, String tag) {
        this.code = code;
        this.tag = tag;
    }

    public String getCode() {
        return code;
    }

    public String getTag() {
        return tag;
    }

}
