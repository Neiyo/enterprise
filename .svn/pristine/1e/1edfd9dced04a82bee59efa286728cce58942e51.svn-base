package com.ylife.exception;

/**
 * Created by InThEnd on 2016/4/11.
 * <p/>
 * 客户访问记录异常。
 */
public class AcessRecordException extends RuntimeException {

    public static final String message = "记录访问错误。";

    public AcessRecordException() {
        super(message);
    }

    public AcessRecordException(String message) {
        super(message);
    }

    public AcessRecordException(String message, Throwable cause) {
        super(message, cause);
    }

    public AcessRecordException(Throwable cause) {
        super(cause);
    }

}
