
#index_head{
    width: 100%;
    height: 70px;
    position: relative;
    margin-top: 20px;
}

#index_head span.left{
    display:inline-block;
    vertical-align: middle;
    width: 220px;
    height: 70px;
    position: absolute;
    left: 120px;
    top: 0px;
    color:#fff;
    font-size: 33px;

}
#index_head span.left img{
    width: 60px;
    height: 60px;

}

#index_head span.right{
    display:inline-block;
    vertical-align: middle;
    width: 320px;
    height: 70px;
    position: absolute;
    right: 120px;
    top: 0px;
}
#index_head span.right img{
    width: 48px;
    height: 48px;
}
#index_head span.right a{
    display:inline-block;
    vertical-align: middle;
    width: 220px;
    height: 70px;
    line-height: 35px;
    font-size: 20px;
    color: #fff;
    opacity: 0.8;
}
