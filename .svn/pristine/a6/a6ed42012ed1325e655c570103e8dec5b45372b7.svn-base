/*------          list.html      -----*/
.containe #notice{
  background-color: #24b35f;
  height:45px;
  line-height: 45px;
  width: 120%;
  margin-left: 20px;
  margin-top: 12px;
  border-radius: 5px;
  font-size: 13px;
  text-align: left;
  padding-left: 18px;
    color: #fff;
}

.table_list th,td{
  height:40px;
  border-radius: 3px;
  padding-left: 5px;
  font-weight: bold;
}

.table_list tr:nth-child(2n){
  background-color: #e7e7e7;
}
.table_list tr:nth-child(2n-1){
  background-color: #f3f3f3;
}
.table_list tr th:nth-of-type(1){
  height:40px;
  width: 100px;
  border-radius: 3px;
}
.table_list tr td:nth-of-type(1){
  height:40px;
  width: 100px;
  border-radius: 3px;
  padding-left: 15px;
}
.table_list tr td:nth-of-type(2){
  height:40px;
  width:300px;
  border-radius: 3px;
  padding-left: 5px;
}
.table_list tr td:nth-of-type(3){
  height:40px;
  width:200px;
  border-radius: 3px;
  padding-left: 5px;
}

