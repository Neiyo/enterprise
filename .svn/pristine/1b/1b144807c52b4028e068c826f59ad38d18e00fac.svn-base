input,textarea:focus {
    outline: none!important;
}
body, ul, li, dl, dt, dd, h1, h2, h3, h4, h5, h6, p,abbr,span,i, form, div, input, textarea {
    margin: 0;
    padding: 0;
}
body {
    background:#fcfcfc;
    color: #666666;
    font-family: "Arial","微软雅黑";
    font-size: 16px;
	width:95%;
	height:100%;
}
h1, h2, h3, h4, h5, h6{
    font-weight: normal;
    font-size: 12px;
}
ul, li {
    list-style-type: none;
}
img {
    border: medium none;
}
img, input, select {
    vertical-align: middle;
}
input, textarea, button {
    font-family:"微软雅黑";
    font-size: 16px;
}
input[type='button']{
    cursor: pointer;
}
a {
    text-decoration: none;
    cursor:pointer;

}
i{
    cursor: pointer;
}
a:hover {
    color: #FF3300;
    text-decoration: none;
}
.topNews{
	width:100%;
	height:53px;
    background:#54b3e3;
	margin-top:0px;
    border-bottom: 1px  solid #046b2f;
	position:relative;
	font-size:16px;
    color: #fff;
    background-size: 100%;
}
.topNews div.spanright{
    position: absolute;
    right: 5%;
    top:10px;
}
.topNews span.news{
	display:inline-block;
	padding:5px;
	font-weight:bold;
    cursor: pointer;
}
.topNews span.news i{
	display:inline-block;
	width:20px;
	height:20px;
    border-radius: 25px;
    background: #ff3300;
	position:relative;
	top:-10px;
	left:-4px;
	color:#FFF;
	font-size:12px;
	font-weight:bold;
	text-align:center;
	line-height:20px;
	font-style:normal;
}

.topNews span.users{
	display:inline-block;
	padding:5px;
		font-weight:bold;
    cursor: pointer;
}
.topNews span.users i{
	display:inline-block;
	width:10px;
	height:10px;
	background:url(../img/arrow.png) center no-repeat;
	margin-left:5px;
}

/*   -------             仓库查询组件   ——————————*/
.chooeseDepot{
    width: 600px;
    height: auto;
    overflow: hidden;
    position: absolute;
    left: 10%;
    top: 15%;
    border: 1px solid #bbb;
    z-index: 3;
    background: #fff;
    display: none;
}
.chooeseDepot div:first-child{
    height: 60px;
    line-height: 60px;
    font-size: 24px;
    text-indent: 20px;
    border-bottom: 1px solid #bbb;
}
.chooeseDepot div:first-child i{
    display:inline-block;
    width:25px;
    height:25px;
    background:url(../img/XX.png) center no-repeat;
    position:relative;
    left:440px;
    top:5px;
    cursor:pointer;
}
.chooeseDepot div:nth-child(2){
    height: 500px;
    overflow-y:scroll ;
}
.chooeseDepot div:nth-child(2) ul{
    height: auto;
    overflow: hidden;
    padding-left: 20px;
    margin-left: 20px;
}
.chooeseDepot div:nth-child(2) ul li{
    padding: 5px 0px;
}
.chooeseDepot div:nth-child(2) ul li a{
    display: inline-block;
    text-align: center;
    width: 15px;
    height: 15px;
    line-height: 15px;
    border-radius: 15px;
    border: 1px solid #bbbbbb;
    margin-right: 10px;

}
.DepotDetail li span{
    cursor: pointer;
}
.DepotDetail li span:hover{
    background: #f1f1f1;
    box-shadow: 0px 1px 3px 0px #bbb;

}
.chooeseDepot input[type="button"]{
    width: 100px;
    height: 40px;
    color: #fff;
    border: none;
    border-radius: 3px;
    margin-top: 30px;
    margin-bottom: 30px;
}
.chooeseDepot input[type="button"]:first-child{
    margin-left: 180px;
}
.chooeseDepot input[type="button"]:nth-child(2){
    background: #24b35f;
    margin-left: 20px;
}

#Dopet{
    padding-left: 5px;
}



/*修改所有页面展示效果*/
.allseachButton{
    width: 120px!important;
    height: 32px!important;
    color: #ffffff!important;
    background: #54a6de!important;
    border: none!important;
    border-radius: 16px!important;
    font-size: 14px!important;
}
.allseachButton:hover,.allseachButton:active{
    background: #2c97de!important;
}
.allinputButton{
    height: 32px!important;
    border-radius: 16px!important;
    text-align: center;
    border: 1px solid #b2b2b2!important;
}
.allclickButton{
    height: 32px!important;
    border: none!important;
    border-radius: 16px!important;
    width: 140px!important;
    color: #ffffff!important;
    font-size: 14px!important;
    background: #f56c73!important;
}
.allclickButton:hover,.allclickButton:active{
    background: #f5454d!important;
}
.allcpageTurnButton{}
.allcpageTurnButton a{
    margin: 0px 10px!important;
    color: #b2b2b2!important;
}
.allcpageTurnButton a:hover{
    color: #54a6de!important;
}
.allcpageTurnButton a.jp-current{
    color: #54a6de !important;
}
.allcpageTurnButton a.jp-next,.allcpageTurnButton a.jp-previous{
    display: inline-block;
    color: #ffffff!important;
    width: 24px;
    height: 24px;
    line-height: 24px;
    text-align: center;
    border-radius: 24px;
    background: #54a6de;
}
.allcpageTurnButton a.jp-next:hover,.allcpageTurnButton a.jp-previous:hover{
    background: #2c97de;
}
.allcpageTurnButton a.jp-disabled{
    color: #cccccc!important;
    background: #f5f5f5!important;
    
}
.allcancelButton{
    width: 120px!important;
    height: 32px!important;
    color: #ffffff!important;
    background: #b2b2b2!important;
    border: none!important;
    border-radius: 16px!important;
    font-size: 14px!important;

}
.allpop{}
.allpop h1{
    text-indent: 30px!important;
    font-size: 16px!important;
    background: #54a6de!important;
    color: #ffffff!important;
    height: 50px!important;
    line-height: 50px!important;
}
@media screen and ( max-width: 1360px){
    .xdsoft_datetimepicker{
        zoom: 120%!important;
    }
    select option{
       font-size:20px ;
    }
    .accountSearch li.allinputButton{
        width: 300px!important;
    }
    .billButton li.allinputButton{
        width: 300px!important;
    }
}
.allpop h1 i{
    position: absolute!important;
    right: 5% !important;
    cursor: pointer!important;
    top: 10px!important;
}
.allSelectButton{
    color: #54a6de!important;
    background: #ffffff!important;
    border: none!important;
}
.allSelectButton:hover{
    background: #2c97de!important;
    color: #ffffff!important;
}
.allheadstyle{
    width: 120%;
    height: 70px;
    line-height: 70px;
    font-size: 16px;
    background:#f3f3f3 ;
    white-space: nowrap;
    overflow: hidden;
}
.allheadstyle a{
    color: #999999;
    display: inline-block;
    width: 15%;
    text-align: center;
    box-shadow:-3px -3px 3px 0px #dce2e6 inset;
}
.allheadstyle span{
    color: #54a6de;
    background: #fff;
    display: inline-block;
    width: 15%;
    text-align: center;
    font-size: 20px;
    border-radius: 5px 5px 0px 0px;
}
.allheadstyle abbr{
    background:#f3f3f3 ;
    display: inline-block;
    width: 100%;
    vertical-align: middle;
    height: 70px;
    box-shadow:3px -3px 3px 0px #dce2e6 inset;
}
.leftshanow{
    box-shadow:3px -3px 3px 0px #dce2e6 inset!important;
}


