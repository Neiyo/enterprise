@media screen and ( max-width: 130px){
    body{
        zoom:62.5%;
        font-size:10px!important;
    }
}
.list{
	width:100%;
	height:1200px;
	color:#8b9599;
	text-align:center;
    background: #fff;
}
.list span{
	display:inline-block;
    vertical-align: middle;
	font-size:24px;
    padding: 10px;
    width: 100%;
    background: #008738;
}
.list ul{
	font-size:16px;
}
.list ul li i{
    display: inline-block;
    vertical-align: middle;
	width:18px;
	height:18px;
	margin-right:10px;
}
.list ul li#account i{
    background:url(../img/com_btn_account_nor.png) center no-repeat;
}
.list ul li#account:hover i{
    background:url(../img/com_btn_account_hover.png) center no-repeat;
}
