.list ul li{
    margin-top: -1px;
    height: 50px;
    line-height: 50px;
    padding-right: 20px;
    background: #ffffff;
    font-size: 16px;
}
@media screen and (max-width: 105px) {
    .list ul li{
        padding-right: 0px!important;
        font-size: 3px!important;
    }
}

.list ul li:hover{
	background:#edf3f8;

}

.list ul li a{
	color:#8b9599;
	display:inline-block;
}
.list ul li:hover a{
    color: #54a6de!important;
}
.list ul li.clickOn{
    background:#edf3f8;
}
.list ul li.clickOn a{
    color: #2c97de;
}
