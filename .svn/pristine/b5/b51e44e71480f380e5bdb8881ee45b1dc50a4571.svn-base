<style type="text/css">
    @media screen and ( max-width: 1360px){
        body{
            zoom:62.5%;
            font-size:10px!important;
        }
    }
    .itemsButton{
        width:1400px;
        height:140px;
        margin-left:20px;
    }
    .itemsButton li{
        width:240px;
        height:40px;
        border:1px solid #CCC;
        float:left;
        margin-left:20px;
        margin-top:20px;
    }
    .itemsButton li span{
        display:inline-block;
        vertical-align:middle;
        width:90px;
        height:40px;
        line-height:40px;
        background:#f9f9f9;
        border-right:1px solid #CCC;
        text-align:center;
    }
    .itemsButton li input{
        width:148px;
        height:40px;
        border:none;
        background:#FFF;
    }
    .itemsButton li input:hover{
        box-shadow: 1px 0px 5px 0px #ccc;

    }

    .itemsButton li input.invenBut{
        width:140px;
        height:42px;
        background:#24b383;
        color:#FFF;
        border:1px solid transparent;
        font-size:15px;
    }
    .itemsButton li input.invenBut:hover{
        box-shadow: 0px 1px 3px 0px #333;
    }
    .itemsButton li input.invenBut:active{
        background: #009966;
    }
    .seachTableDepot{
        margin-top: 20px;
        margin-left: 20px;
        display: none;
    }
    .insidetable td{
        width: 200px;
        margin-left: -1px;
    }
    .insidetable td:first-child{
        width: 300px;;
    }
    .insidetable{
        margin-left: -1px;
    }
    .insidetable2{
        margin-left: -1px;
    }
    .insidetable2 td{
        width: 200px;
        margin-left: -1px;
        border: 1px solid #bbb;
    }
    .insidetable2 td:first-child{
        width: 300px;;
    }
    .seachTableItem{
        margin-top: 20px;
        margin-left: 20px;
        display: none;
    }
    .seachTableItem tbody td{
         border: 1px solid #bbb;
        margin-left: -1px;
    }

</style>
<body>
<script type="text/javascript">
    var eid = ${enterpriseInfo.enterpriseId};
    var ename = "${enterpriseInfo.enterpriseName}";
</script>

<body style="background: #edf3f8">
<div style="background:#fff;margin-left: 20px; margin-top: 20px;margin-bottom: 20px; color:#666666!important;overflow: hidden;">
    <div style="width:100%; height:70px;line-height:70px;font-size:16px;color:#333333;">
    <#if isEnd == true>
        <a style="box-shadow:-3px -2px 3px 0px #dedede inset;font:20px '黑体'; background: #f3f3f3; color:#b2b2b2; display:inline-block; width:10%;line-height: 70px;text-indent: 30px;" href="InventoryManager">仓库管理</a>
    </#if>
    <#if isTop == true>
        <a style="box-shadow:-3px -2px 3px 0px #dedede inset;font:20px '黑体'; background: #f9f9f9; color:#b2b2b2; display:inline-block; width:10%;line-height: 70px;text-indent: 30px;" href="InventoryManager">仓库管理</a>
    </#if>

        <span style="font:20px '黑体'; background: #ffffff; color:#2c97de; display:inline-block; width:10%;line-height: 70px;text-indent: 30px;">库存查询</span>
    <#if isTop == true||isEnd == true>
        <a style="margin-left: -10px;box-shadow:3px -2px 3px 0px #dedede inset;font:20px '黑体';background:#f2f2f2;text-indent:30px; color:#b2b2b2; display:inline-block;width:79%; line-height: 70px; " href="voucherManager">单据管理</a>
    <#else>
        <a style="margin-left: -10px;box-shadow:3px -2px 3px 0px #dedede inset;font:20px '黑体';background:#f2f2f2;text-indent:30px; color:#b2b2b2; display:inline-block;width:89%; line-height: 70px; " href="voucherManager">单据管理</a>
    </#if>

    </div>

<div class="itemsButton">
    <ul>
        <li style="width: 356px;"><span>仓库</span><input value="${enterpriseInfo.enterpriseName}" style="width: 200px" readonly="readonly" type="text" id="Dopet" data-id="null"/><input value="${enterpriseInfo.enterpriseId}" type="hidden" class="enterpriseIdChoosen"><abbr id="choose" style="background:#999; color:#fff;display:inline-block; vertical-align:middle; width: 58px; height:40px; line-height:40px; text-align:center;cursor: pointer;">选择</abbr></li>
        <li><span>货品编号</span><input type="text" name="itemManager_Number" /></li>
        <li><span>货品名称</span><input type="text" name="itemManager_Name" /></li>
        <li style="border: none; width: 800px;">
            <input type="button" value="查询（按货品）" class="invenBut" id="invenSearchItem" />
            <input style="margin-left: 20px;" type="button" class="invenBut" value="查询（按仓库）" id="invenSearchDepot" />
            <input style="margin-left: 20px;" type="button" class="invenBut" value="导出查询库存" id="invenOut" />
            <#if isEnd == true>
            <#elseif isTop == true>
            <#else>
                <input type="button" value="库存调拨" class="invenBut" id="invenLocation" />
            </#if>
        </li>
    </ul>
</div>

<div class="seachTableDepot">
    <table style="table-layout:fixed;" width="100%" height="500px" border="0"  align="center" cellspacing="0" class="seachTableDepotContent">
        <thead align="center" style="background: #666; color: #fff;">
        <tr style="height: 50px;">
            <td style="width: 200px;">仓库</td>
            <td>
                <table style="table-layout:fixed;" class="insidetable" width="100%" border="0"  align="center" cellspacing="0">
                    <tbody align="center">
                        <tr align="center">
                            <td>货品名称</td>
                            <td>货品规格</td>
                            <td>货品编号</td>
                            <td>库存</td>
                            <td>可用库存</td>
                            <td>品牌</th>
                            <td>所属商家</td>
                        </tr>
                    </tbody>
                </table>
            </td>
        </tr>
        </thead>
        <tbody align="center" class="tbodyContent">

        </tbody>
    </table>
</div>



<div class="seachTableItem">
    <table width="100%" align="center" border="0" cellspacing="0">
        <thead>
            <tr style="height: 50px; background: #666; color: #fff;">
                <th>货品名称</th>
                <th>货品规格</th>
                <th>货品编号</th>
                <th>库存</th>
                <th>可用库存</th>
                <th>品牌</th>
                <th>所属商家</th>
            </tr>
        </thead>
        <tbody align="center" >

        </tbody>
    </table>
</div>


<div class="chooeseDepot"></div>

<div id="cover1"  style="width: 100%; height: 100%; z-index: 1; background: #000; display: none; opacity:0.5;position:fixed; left: 0px; top: 0px;"></div>
    </div>
</body>

<script type="text/javascript">
    $(document).ready(function(){
        //禁止后退键 作用于Firefox、Opera
        document.onkeypress = forbidBackSpace;
        //禁止后退键  作用于IE、Chrome
        document.onkeydown = forbidBackSpace;

        //仓库列表
        inventoryList(eid,ename);

        $(document).on("click","#invenLocation",function(){
              window.open("allocation");
        });

    })
</script>