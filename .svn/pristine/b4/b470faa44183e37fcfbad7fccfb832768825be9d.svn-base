
body, ul, li, dl, dt, dd, h1, h2, h3, h4, h5, h6, p,abbr,span,i, form, div, input, textarea {
    margin: 0;
    padding: 0;
}
body {
    background:#fcfcfc;
    color: #333;
    font-family: "Arial","微软雅黑";
    font-size: 16px;
	width:100%;
	height:100%;
}
h1, h2, h3, h4, h5, h6{
    font-weight: normal;
    font-size: 12px;
}
ul, li {
    list-style-type: none;
}
img {
    border: medium none;
}
img, input, select {
    vertical-align: middle;
}
input, textarea, button {
    font-family:"微软雅黑";
    font-size: 16px;
}
input[type='button']{
    cursor: pointer;
}
a {
    text-decoration: none;
    cursor:pointer;

}
i{
    cursor: pointer;
}
a:hover {
    color: #FF3300;
    text-decoration: none;
}
.topNews{
	width:100%;
	height:53px;
    background:url(../img/com_pic_top_bar.png) center no-repeat;
	margin-top:0px;
    border-bottom: 1px  solid #046b2f;
	position:relative;
	font-size:16px;
    color: #fff;
    background-size: 100%;
}
.topNews div.spanright{
    position: absolute;
    right: 5%;
    top:10px;
}
.topNews span.news{
	display:inline-block;
	padding:5px;
	font-weight:bold;
    cursor: pointer;
}
.topNews span.news i{
	display:inline-block;
	width:20px;
	height:20px;
    border-radius: 25px;
    background: #ff3300;
	position:relative;
	top:-10px;
	left:-4px;
	color:#FFF;
	font-size:12px;
	font-weight:bold;
	text-align:center;
	line-height:20px;
	font-style:normal;
}

.topNews span.users{
	display:inline-block;
	padding:5px;
		font-weight:bold;
    cursor: pointer;
}
.topNews span.users i{
	display:inline-block;
	width:10px;
	height:10px;
	background:url(../img/arrow.png) center no-repeat;
	margin-left:5px;
}
#holder a:hover{
    background: #24b383;
    color: #fff;
}
.holder a:hover{
    background: #24b383;
    color: #fff;
}

/*   -------             仓库查询组件   ——————————*/
.chooeseDepot{
    width: 600px;
    height: auto;
    overflow: hidden;
    position: absolute;
    left: 10%;
    top: 15%;
    border: 1px solid #bbb;
    z-index: 3;
    background: #fff;
    display: none;
}
.chooeseDepot div:first-child{
    height: 60px;
    line-height: 60px;
    font-size: 24px;
    text-indent: 20px;
    border-bottom: 1px solid #bbb;
}
.chooeseDepot div:first-child i{
    display:inline-block;
    width:25px;
    height:25px;
    background:url(../img/XX.png) center no-repeat;
    position:relative;
    left:440px;
    top:5px;
    cursor:pointer;
}
.chooeseDepot div:nth-child(2){
    height: 500px;
    overflow-y:scroll ;
}
.chooeseDepot div:nth-child(2) ul{
    height: auto;
    overflow: hidden;
    padding-left: 20px;
    margin-left: 20px;
}
.chooeseDepot div:nth-child(2) ul li{
    padding: 5px 0px;
}
.chooeseDepot div:nth-child(2) ul li a{
    display: inline-block;
    text-align: center;
    width: 15px;
    height: 15px;
    line-height: 15px;
    border-radius: 15px;
    border: 1px solid #bbbbbb;
    margin-right: 10px;

}
.DepotDetail li span{
    cursor: pointer;
}
.DepotDetail li span:hover{
    background: #f1f1f1;
    box-shadow: 0px 1px 3px 0px #bbb;

}
.chooeseDepot input[type="button"]{
    width: 100px;
    height: 40px;
    color: #fff;
    border: none;
    border-radius: 3px;
    margin-top: 30px;
    margin-bottom: 30px;
}
.chooeseDepot input[type="button"]:first-child{
    margin-left: 180px;
}
.chooeseDepot input[type="button"]:nth-child(2){
    background: #24b35f;
    margin-left: 20px;
}

#Dopet{
    padding-left: 5px;
}