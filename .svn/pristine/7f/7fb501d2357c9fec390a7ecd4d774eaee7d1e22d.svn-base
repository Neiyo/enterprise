.list ul li{
    margin-top: -1px;
    height: 50px;
    line-height: 50px;
    padding-right: 20px;
    border-width: 1px 0px 1px 3px ;
    border-style: solid;
    border-color:#006d26 transparent #006d26 transparent ;
    background: #008738;
    font-size: 16px;
}
@media screen and (max-width: 105px) {
    .list ul li{
        padding-right: 0px!important;
        font-size: 3px!important;
    }
}

.list ul li:hover{
	background:#0b944f;
}

.list ul li a{
	color:#FFF;
